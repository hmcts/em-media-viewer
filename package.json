{
  "name": "@hmcts/media-viewer",
  "version": "4.1.6-angular-19.2.15",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "makefile": "yarn package && yarn start:ng;",
    "start:ng": "ng serve --proxy-config proxy.config.js",
    "start:api": "node ./dist/api/server.js",
    "setup": "yarn setup:api && yarn setup:demo",
    "setup:api": "cross-env NODE_PATH=. rm -rf ./dist/api && tsc -p ./api --outDir dist/api",
    "setup:demo": "yarn build && yarn copy:demo-files",
    "package": "yarn build:lib && yarn copy:lib-files && yarn npm-pack",
    "build": "yarn build:lib && yarn build:demo",
    "build:lib": "cd projects/media-viewer && yarn install && ng build --configuration production",
    "build:demo": "ng build media-viewer-demo-app --configuration production",
    "copy:lib-files": "yarn copy:lib-js-dependencies && yarn copy:lib-assets && yarn copy:docs",
    "copy:demo-files": "yarn copy:demo-js-dependencies && yarn copy:demo-assets",
    "copy:lib-js-dependencies": "cross-env NODE_PATH=. rsync -r projects/media-viewer/node_modules/pdfjs-dist/build dist/media-viewer/assets",
    "copy:demo-js-dependencies": "cross-env NODE_PATH=. rsync -r projects/media-viewer/node_modules/pdfjs-dist/build dist/demo-app/assets",
    "copy:lib-assets": "cross-env NODE_PATH=. rsync -r projects/media-viewer/src/assets/* dist/media-viewer/assets/",
    "copy:demo-assets": "cross-env NODE_PATH=. rsync -r projects/media-viewer/src/assets/* dist/demo-app/assets/",
    "copy:docs": "yarn copy:readme && yarn copy:licence",
    "copy:licence": "cross-env NODE_PATH=. cp ./README.md dist/media-viewer",
    "copy:readme": "cross-env NODE_PATH=. cp ./LICENCE dist/media-viewer",
    "publish:dist": "yarn package && cd dist/media-viewer && yarn install --no-immutable && yarn npm publish",
    "npm-pack": "cd dist/media-viewer && touch yarn.lock && yarn pack",
    "test": "cd projects/media-viewer; ng test",
    "test:coverage": "echo test:coverage",
    "test:a11y": "echo test:a11y",
    "test:smoke": "NODE_PATH=. node ./node_modules/codeceptjs/bin/codecept.js run -c ./test/end-to-end/codecept.smoke.conf.js --grep @smoke --steps --reporter mocha-multi",
    "test:fullfunctional": "yarn e2e:fullfunctional",
    "test:functional": "yarn e2e:fullfunctional",
    "test:crossbrowser_PT": "protractor e2e/saucelabs.conf.js",
    "test:coveralls-upload": "cross-env NODE_PATH=. cat ./reports/tests/coverage/ng/lcov.info | coveralls",
    "test:codecov-upload": "codecov -f ./reports/tests/coverage/ng/lcov.info",
    "test:pa11y": "pa11y-ci",
    "lint": "ng lint",
    "clean": "rimraf reports",
    "stub:annotations": "node api/annotation-stubs.js &",
    "webdriver:update": "webdriver-manager update --versions.chrome 2.44",
    "e2e": "protractor --webdriverUpdate=false",
    "e2e:local": "yarn webdriver:update && protractor e2e/local.protractor.conf.js",
    "e2e:fullfunctional_PT": "yarn webdriver:update && protractor e2e/ci.protractor.conf.js",
    "e2e:fullfunctional": "NODE_PATH=. node ./node_modules/codeceptjs/bin/codecept.js run -c ./test/end-to-end/ --grep @ci --steps --reporter mocha-multi",
    "sonar-scan": "node_modules/sonar-scanner/bin/sonar-scanner",
    "rebuild-node-sass": "yarn add node-sass@^4.14.1",
    "test:crossbrowser": "NODE_PATH=. codeceptjs run-multiple ${BROWSER_GROUP:-'--all'} -c test/end-to-end/saucelabs.conf.js --plugins retryFailedStep --steps --grep '@xb' --reporter mocha-multi"
  },
