{
  "name": "@hmcts/media-viewer",
  "version": "2.6.9",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ng": "ng serve --proxy-config proxy.config.js",
    "start:vm": "ng serve --disable-host-check --host 0.0.0.0",
    "start:api": "node ./dist/api/server.js",
    "setup": "yarn setup:api && yarn setup:demo",
    "setup:api": "cross-env NODE_PATH=. rm -rf ./dist/api && tsc -p ./api --outDir dist/api",
    "setup:demo": "yarn build && yarn copy:demo-files",
    "build": "yarn build:lib && yarn build:demo",
    "build:lib": "cd projects/media-viewer && yarn install && ng build --prod",
    "build:demo": "ng build media-viewer-demo-app --prod",
    "copy:lib-files": "yarn copy:lib-js-dependencies && yarn copy:lib-assets && yarn copy:docs",
    "copy:demo-files": "yarn copy:demo-js-dependencies && yarn copy:demo-assets",
    "copy:lib-js-dependencies": "cross-env NODE_PATH=. rsync -r projects/media-viewer/node_modules/pdfjs-dist/build dist/media-viewer/assets",
    "copy:demo-js-dependencies": "cross-env NODE_PATH=. rsync -r projects/media-viewer/node_modules/pdfjs-dist/build dist/demo-app/assets",
    "copy:lib-assets": "cross-env NODE_PATH=. rsync -r projects/media-viewer/src/assets/* dist/media-viewer/assets/",
    "copy:demo-assets": "cross-env NODE_PATH=. rsync -r projects/media-viewer/src/assets/* dist/demo-app/assets/",
    "copy:docs": "yarn copy:readme && yarn copy:license",
    "copy:license": "cross-env NODE_PATH=. cp ./README.md dist/media-viewer",
    "copy:readme": "cross-env NODE_PATH=. cp ./LICENSE dist/media-viewer",
    "publish:dist": "yarn package && cd dist/media-viewer && yarn publish",
    "package": "yarn build:lib && yarn copy:lib-files && yarn npm-pack",
    "npm-pack": "cd dist/media-viewer && yarn pack",
    "test": "cd projects/media-viewer; ng test",
    "test:coverage": "yarn test",
    "test:a11y": "yarn test",
    "test:smoke": "yarn test",
    "test:functional": "yarn e2e:functional",
    "test:codacy-upload": "cross-env NODE_PATH=. cat ./reports/tests/coverage/ng/lcov.info | codacy-coverage",
    "test:coveralls-upload": "cross-env NODE_PATH=. cat ./reports/tests/coverage/ng/lcov.info | coveralls",
    "test:codecov-upload": "codecov -f ./reports/tests/coverage/ng/lcov.info",
    "test:pa11y": "pa11y-ci",
    "lint": "ng lint",
    "clean": "rimraf reports",
    "stub:annotations": "node api/annotation-stubs.js &",
    "webdriver:update": "webdriver-manager update --versions.chrome 2.44",
    "e2e": "protractor --webdriverUpdate=false",
    "e2e:local": "yarn webdriver:update && protractor e2e/local.protractor.conf.js",
    "e2e:functional": "yarn webdriver:update && protractor e2e/ci.protractor.conf.js",
    "sonar-scan": "node_modules/sonar-scanner/bin/sonar-scanner",
    "rebuild-node-sass": "npm install node-sass@4.14.1"
  },
  "dependencies": {
    "angular-tree-component": "^8.5.6",
    "@swimlane/ngx-datatable": "^16.0.2",
    "hammerjs": "^2.0.8",
    "mutable-div": "0.0.11",
    "pdfjs-dist": "2.5.207",
    "uuid": "^3.4.0",
    "socket.io-client": "^3.1.1",
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/cli": "^7.3.9",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/forms": "^7.2.15",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/platform-server": "^7.2.15",
    "@angular/router": "^7.2.15",
    "@hmcts/frontend": "^0.0.39-alpha",
    "@hmcts/nodejs-healthcheck": "^1.7.0",
    "@hmcts/properties-volume": "^0.0.9",
    "@ngrx/effects": "^7.4.0",
    "@ngrx/router-store": "^7.4.0",
    "@ngrx/store": "^7.4.0",
    "@ngrx/store-devtools": "^7.4.0",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "config": "^3.3.4",
    "core-js": "^2.6.5",
    "exception-formatter": "^2.1.2",
    "formidable": "^1.2.1",
    "glob": "^7.1.4",
    "global-tunnel-ng": "^2.7.1",
    "govuk-frontend": "^3.11.0",
    "moment-timezone": "^0.5.28",
    "ngx-chips": "^1.9.8",
    "nodemon": "^1.19.0",
    "nunjucks": "^3.2.0",
    "otp": "^0.1.3",
    "rxjs": "^6.5.2",
    "rxjs-compat": "^6.5.2",
    "scss-bundle": "~2.4.0",
    "session-file-store": "^1.2.0",
    "zone.js": "^0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.10",
    "@angular-devkit/build-ng-packagr": "^0.13.10",
    "@angular-devkit/core": "^7.3.9",
    "@angular/compiler-cli": "^7.2.15",
    "@hmcts/eslint-config": "^1.4.0",
    "@ngrx/schematics": "^8.6.0",
    "@types/chai": "^4.2.15",
    "@types/chai-as-promised": "^7.1.3",
    "@types/cucumber": "^7.0.0",
    "@types/jasmine": "^3.6.4",
    "@types/jasminewd2": "^2.0.8",
    "@types/mocha": "^8.2.1",
    "@types/node": "12.0.8",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "chai-smoothie": "^0.3.2",
    "circular-dependency-plugin": "^5.0.2",
    "codacy-coverage": "^3.4.0",
    "codecov": "^3.7.1",
    "codelyzer": "^5.0.1",
    "coveralls": "^3.0.3",
    "cross-env": "^7.0.3",
    "cucumber": "5.1.0",
    "cucumber-html-report": "0.6.5",
    "cucumber-html-reporter": "^4.0.5",
    "cucumber-pretty": "1.5.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "istanbul-instrumenter-loader": "3.0.1",
    "jasmine": "^3.6.4",
    "jasmine-core": "^3.6.0",
    "jasmine-marbles": "^0.8.1",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^6.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-phantomjs-launcher": "^1.0.4",
    "ng-packagr": "~4.7.1",
    "ngrx-store-freeze": "^0.2.4",
    "pa11y-ci": "^2.4.0",
    "protractor": "^5.4.2",
    "protractor-cucumber-framework": "^6.1.1",
    "protractor-multiple-cucumber-html-reporter-plugin": "1.8.0",
    "sonar-scanner": "^3.1.0",
    "ts-node": "^8.1.0",
    "tsickle": "^0.35.0",
    "tslib": "^1.9.3",
    "tslint": "^5.20.1",
    "typescript": ">=3.1.1 <3.3",
    "typescript-string-operations": "^1.3.1",
    "webdriver-manager": "^12.1.7"
  }
}
