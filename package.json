{
  "name": "@hmcts/media-viewer",
  "version": "2.6.7-RC",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ng": "ng serve --proxy-config proxy.config.js",
    "start:vm": "ng serve --disable-host-check --host 0.0.0.0",
    "build": "yarn build:lib && ng build --prod",
    "copy:license": "cp ./README.md dist/media-viewer",
    "copy:readme": "cp ./LICENSE dist/media-viewer",
    "copy:docs": "yarn copy:readme && yarn copy:license",
    "copy:assets": "yarn copy:lib-assets",
    "copy:lib-assets": "rsync -r projects/media-viewer/src/assets/* dist/media-viewer/assets/",
    "install:js-dependencies": "cd projects/media-viewer && yarn install",
    "copy:js-dependencies": "rsync -r projects/media-viewer/node_modules/pdfjs-dist/build dist/media-viewer/assets",
    "copy:files": "yarn copy:js-dependencies && yarn copy:assets && yarn copy:docs",
    "build:lib": "yarn install && yarn install:js-dependencies && cd projects/media-viewer && ng build --prod",
    "npm-pack": "cd dist/media-viewer && yarn pack",
    "package": "yarn build:lib && yarn copy:files && yarn npm-pack",
    "publish:dist": "yarn package && cd dist/media-viewer && yarn publish",
    "test": "cd projects/media-viewer; ng test",
    "test:coverage": "yarn test",
    "test:a11y": "yarn test",
    "test:smoke": "yarn test",
    "test:functional": "yarn test",
    "test:codacy-upload": "cat ./reports/tests/coverage/ng/lcov.info | codacy-coverage",
    "test:coveralls-upload": "cat ./reports/tests/coverage/ng/lcov.info | coveralls",
    "test:codecov-upload": "codecov -f ./reports/tests/coverage/ng/lcov.info",
    "test:pa11y": "pa11y-ci",
    "lint": "ng lint",
    "clean": "rimraf reports",
    "stub:annotations": "node api/annotation-stubs.js &",
    "webdriver:update": "webdriver-manager update --versions.chrome 77.0.3865.40 --versions.gecko v0.25.0",
    "e2e": "yarn stub:annotations && ng e2e --webdriverUpdate=false",
    "e2e:cucumber": "yarn webdriver:update && yarn e2e -- --protractor-config e2e/cucumber.conf.js",
    "e2e:ci": "yarn webdriver:update && yarn stub:annotations && ng e2e --webdriverUpdate=false --protractor-config 'e2e/ci.protractor.conf.js'"
  },
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/cli": "^7.3.9",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/forms": "^7.2.15",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/platform-server": "^7.2.15",
    "@angular/router": "^7.2.15",
    "@hmcts/frontend": "^0.0.39-alpha",
    "@ngrx/effects": "^7.4.0",
    "@ngrx/router-store": "^7.4.0",
    "@ngrx/store": "^7.4.0",
    "@ngrx/store-devtools": "^7.4.0",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "body-parser": "^1.19.0",
    "core-js": "^2.6.5",
    "exception-formatter": "^2.1.2",
    "formidable": "^1.2.1",
    "glob": "^7.1.4",
    "global-tunnel-ng": "^2.7.1",
    "govuk-frontend": "^3.11.0",
    "moment-timezone": "^0.5.28",
    "ngx-chips": "^1.9.8",
    "nodemon": "^1.19.0",
    "rxjs": "^6.5.2",
    "rxjs-compat": "^6.5.2",
    "scss-bundle": "~2.4.0",
    "session-file-store": "^1.2.0",
    "zone.js": "^0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.10",
    "@angular-devkit/build-ng-packagr": "^0.13.10",
    "@angular-devkit/core": "^7.3.9",
    "@angular/compiler-cli": "^7.2.15",
    "@hmcts/eslint-config": "^1.4.0",
    "@ngrx/schematics": "^8.6.0",
    "@types/chai": "3.4.34",
    "@types/chai-as-promised": "0.0.29",
    "@types/cucumber": "~4.0.5",
    "@types/jasmine": "^3.3.13",
    "@types/jasminewd2": "^2.0.6",
    "@types/mocha": "^5.2.6",
    "@types/node": "12.0.8",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-smoothie": "^0.3.2",
    "circular-dependency-plugin": "^5.0.2",
    "codacy-coverage": "^3.4.0",
    "codecov": "^3.7.1",
    "codelyzer": "^5.0.1",
    "coveralls": "^3.0.3",
    "cucumber": "^5.1.0",
    "cucumber-html-reporter": "^5.0.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "istanbul-instrumenter-loader": "3.0.1",
    "jasmine": "^3.4.0",
    "jasmine-core": "^3.4.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "ng-packagr": "~4.7.1",
    "ngrx-store-freeze": "^0.2.4",
    "pa11y-ci": "^2.4.0",
    "protractor": "^5.4.2",
    "protractor-cucumber-framework": "^6.1.1",
    "ts-node": "^8.1.0",
    "tsickle": "^0.35.0",
    "tslib": "^1.9.3",
    "tslint": "^5.20.1",
    "typescript": ">=3.1.1 <3.3",
    "typescript-string-operations": "^1.3.1",
    "webdriver-manager": "^12.1.7"
  }
}
