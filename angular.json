{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "media-viewer": {
      "root": "projects/media-viewer",
      "sourceRoot": "projects/media-viewer/src",
      "projectType": "library",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "projects/media-viewer/tsconfig.lib.json",
            "project": "projects/media-viewer/ng-package.json",
            "stylePreprocessorOptions": {
              "includePaths": [
                "node_modules/govuk_frontend_toolkit/stylesheets",
                "node_modules/govuk-elements-sass/public/sass"
              ]
            },
            "assets": [
              {
                "glob": "**/*",
                "input": "node_modules/@hmcts/media-viewer/assets",
                "output": "/assets"
              },
              "src/favicon.ico",
              "src/assets",
              "src/web.config"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "project": "projects/media-viewer/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "codeCoverage": true,
            "codeCoverageExclude": [
              "**/*.spec.ts",
              "**/*.spec.js",
              "test/**",
              "config/**",
              "api/**/templates/**",
              "src/public/**",
              "src/environments/**"
            ],
            "main": "projects/media-viewer/src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "projects/media-viewer/tsconfig.spec.json",
            "karmaConfig": "projects/media-viewer/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/media-viewer/tsconfig.lib.json",
              "projects/media-viewer/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "media-viewer-demo-app": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/demo-app",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "dist/media-viewer/assets",
                "output": "/assets"
              },
              {
                "glob": "**/*",
                "input": "dist/media-viewer/assets/images",
                "output": "/"
              },
              {
                "glob": "favicon.ico",
                "input": "src",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              },
              {
                "glob": "**/*",
                "input": "src/assets/documents/pdf",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "src/assets/documents/image",
                "output": "/"
              }
            ],
            "styles": [],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "media-viewer-demo-app:build",
            "port": 3000,
            "proxyConfig": "proxy.config.js"
          },
          "configurations": {
            "production": {
              "browserTarget": "media-viewer-demo-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "media-viewer-demo-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "codeCoverage": true,
            "codeCoverageExclude": [
              "**/*.spec.ts",
              "**/*.spec.js",
              "test/**",
              "config/**",
              "api/**/templates/**",
              "src/public/**",
              "src/environments/**"
            ],
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "media-viewer-e2e": {
      "root": "projects/media-viewer-e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "media-viewer-demo-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "media-viewer-demo-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "media-viewer",
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "scss"
    }
  }
}
